{
  "name": "mocean/symfony-mocean-bundle",
  "description": "Mocean API for Symfony Framework",
  "type": "library",
  "keywords": [
    "sms",
    "verify",
    "symfony",
    "framework",
    "mocean"
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "symfony/framework-bundle": "^3.0.0|^4.0.0",
    "mocean/client": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "autoload": {
    "psr-4": {
      "MoceanSymBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MoceanSymBundle\\Tests\\": "tests/"
    }
  }
}
